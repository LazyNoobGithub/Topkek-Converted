-- How to find filtering enabled, skidded by LazyNoobRobloxian. / LazyNoob#0001

local workspacegame = game:GetService("Workspace")
local filteringvarible = workspacegame.FilteringEnabled
local message = "Game is: "
local FE = "Filtering Enabled"
local FD = "Filtering Disabled"

function GetFiltering() if filteringvarible == true then return message.. FE else return message.. FD end end
function ReturnFiltering() local FilteringThing = GetFiltering() return FilteringThing end

function Notify(main,desc,icon,dur)
    --rbxthumb://type=Asset&id=5107182114&w=150&h=150
    --rbxthumb://type=Asset&id=408308385&w=150&h=150
     game:GetService("StarterGui"):SetCore("SendNotification",{Title = main, Text = desc, Icon = "rbxthumb://type=Asset&id="..icon.. "&w=150&h=150", Duration = dur})
end
function GetMessage()
   local TheMessage = [[Convert By LazyNoob | Reanimate By Gelatek
   Grabknife by mustardfoot and Tollonis]] return TheMessage 
end
function GetImage()
    local Image = 12239869496
    return Image
end
Notify("Reanimation","Reanimating [1/3]",GetImage(),3)
wait(3)
local global_env = (getgenv and getgenv()) or _G
global_env.Settings = {
    ["DisableFlingingOnTouch"] = false,
    ["KeepWeldedHair"] = true,
    ["Headless"] = false,
    ["PermaDeath"] = false,
    ["R15ToR6"] = true,
    ["EnableAnims"] = true,
    ["FullNoclip"] = false,
    ["AntiVoid"] = true,
    ["Bullet"] = false,
}
loadstring(game:HttpGet("https://raw.githubusercontent.com/toldblock/Reanimation/main/main.lua"))()
Notify("Reanimation","Reanimating [2/3]",GetImage(),3)

wait(5)

Notify("Reanimation","Reanimating [3/3]",GetImage(),3)
